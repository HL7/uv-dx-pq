            <!-- #85 -->
            <goal id="goal-control-pH">
                <category>
                    <coding>
                        <system value="http://accumulus.org/fhir/code/testCategory"/>
                        <code value="C123456"/>
                        <display value="Physical Property"/>
                    </coding>
                </category>
                <description>
                    <text value="Controls - pH"/>
                    <!-- this is mandatory, even though we don't really want it -->
                </description>
                <priority>
                    <coding>
                        <system value="http://accumulus.org/fhir/code/goalPriority"/>
                        <code value="critical"/>
                        <display value="Critical"/>
                    </coding>
                </priority>
                <target>
                    <measure>
                        <coding>
                            <system value="http://dummy.loinc.org"/>
                            <code value="PH"/>
                            <display value="pH Level"/>
                        </coding>
                        <text value="pH must be in range 6.9 to 7.1"/>
                    </measure>
                    <!--detailString value="can also be a string"-->
                    <detailRange>
                        <low>
                            <value value="6.9"/>
                            <unit value="pH"/>
                        </low>
                        <high>
                            <value value="7.1"/>
                            <unit value="pH"/>
                        </high>
                    </detailRange>
                </target>
            </goal>
            <goal id="goal-control-water-content">
                <category>
                    <coding>
                        <system value="http://accumulus.org/fhir/code/testCategory"/>
                        <code value="C123456"/>
                        <display value="Physical Property"/>
                    </coding>
                </category>
                <description>
                    <text value="Controls - Water Content"/>
                    <!-- this is mandatory, even though we don't really want it -->
                </description>
                <priority>
                    <coding>
                        <system value="http://accumulus.org/fhir/code/goalPriority"/>
                        <code value="critical"/>
                        <display value="Critical"/>
                    </coding>
                </priority>
                <target>
                    <measure>
                        <coding>
                            <system value="http://dummy.loinc.org"/>
                            <code value="WaterContent"/>
                            <display value="Water content"/>
                        </coding>
                        <text value="Under 2%"/>
                    </measure>
                    <!--detailString value="can also be a string"-->
                    <detailRange>
                        <high>
                            <value value="2"/>
                            <unit value="%"/>
                        </high>
                    </detailRange>
                </target>
            </goal>
            <goal id="goal-control-particle-size">
                <category>
                    <coding>
                        <system value="http://accumulus.org/fhir/code/testCategory"/>
                        <code value="C123456"/>
                        <display value="Physical Property"/>
                    </coding>
                </category>
                <description>
                    <text value="Controls - Particle Size"/>
                </description>
                <priority>
                    <coding>
                        <system value="http://accumulus.org/fhir/code/goalPriority"/>
                        <code value="critical"/>
                        <display value="Critical"/>
                    </coding>
                </priority>
                <target>
                    <measure>
                        <coding>
                            <system value="http://dummy.loinc.org"/>
                            <code value="ParticleSize"/>
                            <display value="Particle Size"/>
                        </coding>
                        <text value="Under 200 microns"/>
                    </measure>
                    <!--detailString value="can also be a string"-->
                    <detailRange>
                        <high>
                            <value value="200"/>
                            <unit value="microns"/>
                        </high>
                    </detailRange>
                </target>
            </goal>
