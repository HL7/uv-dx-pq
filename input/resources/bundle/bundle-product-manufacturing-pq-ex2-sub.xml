   <id value="bundle-product-manufacturing-pq-ex2-sub"/>
<!--   <id value="product-manufacturing"/>-->
   <meta>
<!--      <profile value="http://accumulus.org/fhir/dx-cmc/manufacturing"/>-->
      <profile value="http://hl7.org/fhir/uv/pharm-quality/StructureDefinition/Bundle-product-manufacturing-pq"/>
   </meta>
   <type value="collection"/>
   <entry>
      <fullUrl value="urn:uuid:bf6b038b-1334-3dd4-a552-65c169e2265a"/>
      <resource>
         <PlanDefinition>
            <id value="manufacturingProcess-unlinked"/>
            <extension url="http://hl7.org/fhir/uv/pharm-quality/StructureDefinition/Extension-plan-document-reference-pq">
               <valueReference>
                        <!-- Process flow diagram -->
                        <!-- or could be a Binary -->
                        <!-- #84 -->
                  <reference value="DocumentReference/documentreference-drug-pq-process-flow-diagram"/>
               </valueReference>
            </extension>
            <extension url="http://hl7.org/fhir/uv/pharm-quality/StructureDefinition/Extension-history-of-process-pq">
               <valueMarkdown value="This can be text about the history"/>
            </extension>
            <url value="http://example-server.com/fhir/PlanDefinition/bf6b038b-1334-3dd4-a552-65c169e2265a"/>
            <type>
               <coding>
                  <system value="http://accumulus.org/fhir/code/planType"/>
                  <code value="1244566"/>
                  <display value="Manufacturing Process"/>
               </coding>
            </type>
            <status value="active"/>
            <!-- #83 -->
            <subjectReference>
               <reference value="SubstanceDefinition/substance1"/>
            </subjectReference>
            <description value="Narrative description of substance manufacturing process"/>
            <!-- #85 -->
            <goal id="goal-control-pH">
               <category>
                  <coding>
                     <system value="http://accumulus.org/fhir/code/testCategory"/>
                     <code value="C123456"/>
                     <display value="Physical Property"/>
                  </coding>
               </category>
               <description>
                  <text value="Controls - pH"/>
                  <!-- this is mandatory, even though we don't really want it -->
               </description>
               <priority>
                  <coding>
                     <system value="http://accumulus.org/fhir/code/goalPriority"/>
                     <code value="critical"/>
                     <display value="Critical"/>
                  </coding>
               </priority>
               <target>
                  <measure>
                     <coding>
                        <system value="http://dummy.loinc.org"/>
                        <code value="PH"/>
                        <display value="pH Level"/>
                     </coding>
                     <text value="Long description of the acceptance criteria - pH must be in range 6.9 to 7.1"/>
                  </measure>
                  <!--detailString value="can also be a string"-->
                  <detailRange>
                     <low>
                        <value value="6.9"/>
                        <unit value="pH"/>
                     </low>
                     <high>
                        <value value="7.1"/>
                        <unit value="pH"/>
                     </high>
                  </detailRange>
               </target>
            </goal>
            <goal id="goal-control-temp">
               <category>
                  <coding>
                     <system value="http://accumulus.org/fhir/code/testCategory"/>
                     <code value="C123456"/>
                     <display value="Physical Property"/>
                  </coding>
               </category>
               <description>
                  <text value="Controls - Temperature"/>
                  <!-- this is mandatory, even though we don't really want it -->
               </description>
               <priority>
                  <coding>
                     <system value="http://accumulus.org/fhir/code/goalPriority"/>
                     <code value="noncritical"/>
                     <display value="Non Critical"/>
                  </coding>
               </priority>
               <target>
                  <measure>
                     <coding>
                        <system value="http://dummy.loinc.org"/>
                        <code value="TEMP"/>
                        <display value="Temperature"/>
                     </coding>
                     <text value="Long description of the control value - Temperature must be below 150°C"/>
                  </measure>
                  <!--detailString value="can also be a string"-->
                  <detailRange>
                     <high>
                        <value value="150"/>
                        <unit value="°C"/>
                        <system value="http://unitsofmeasure.org"/>
                        <code value="Cel"/>
                     </high>
                  </detailRange>
               </target>
            </goal>
            <!-- outer action is the whole thing, if needed -->
            <action>
               <code>
                  <coding>
                     <system value="http://accumulus.org/fhir/code/manufacturingStep"/>
                     <code value="overall process"/>
                  </coding>
                  <text value="overall process"/>
               </code>
               <action>
                  <extension url="http://hl7.org/fhir/uv/pharm-quality/StructureDefinition/Extension-process-step-scale-pq">
                     <valueCoding>
                        <system value="http://accumulus.org/fhir/code/manufacturingStepScale"/>
                        <code value="ABCD"/>
                        <display value="Production Scale"/>
                     </valueCoding>
                  </extension>
                  <extension url="http://hl7.org/fhir/uv/pharm-quality/StructureDefinition/Extension-process-parameters-pq">
                     <extension url="parameter">
                        <valueCodeableConcept>
                           <coding>
                              <system value="http://accumulus.org/fhir/code/processParameter"/>
                              <code value="SPEED"/>
                              <display value="Mix speed"/>
                           </coding>
                        </valueCodeableConcept>
                     </extension>
                     <extension url="value">
                        <valueQuantity>
                           <value value="1500"/>
                           <unit value="rpm"/>
                        </valueQuantity>
                     </extension>
                  </extension>
                  <prefix value="1"/>
                  <description value="Description of this step"/>
                  <code>
                     <coding>
                        <system value="http://accumulus.org/fhir/code/manufacturingStep"/>
                        <code value="2222"/>
                        <display value="Particle Size Reduction"/>
                     </coding>
                     <!--text value="Step 2 in the process - free text is possible"/-->
                  </code>
                  <documentation>
                     <type value="specification-of"/>
                     <resourceReference>
                        <reference value="DocumentReference/DocumentReference-image"/>
                     </resourceReference>
                  </documentation>
                  <goalId value="goal-control-pH"/>
                  <goalId value="goal-control-temp"/>
                  <participant>
                     <typeReference>
                                <!-- this is new in R5 and is not in R4B -->
                        <reference value="DeviceDefinition/impeller"/>
                     </typeReference>
                     <role>
                        <coding>
                           <system value="http://accumulus.org/fhir/code/equipmentClass"/>
                           <code value="example"/>
                           <!-- #88 Equipment class -->
                           <display value="Compression Milling"/>
                        </coding>
                     </role>
                  </participant>
                  <participant>
                     <typeReference>
                        <extension url="http://hl7.org/fhir/uv/pharm-quality/StructureDefinition/Extension-manufacturing-participant-pq">
                           <valueReference>
                              <reference value="SubstanceDefinition/startingMaterial"/>
                           </valueReference>
                        </extension>
                     </typeReference>
                     <role>
                        <coding>
                           <system value="http://accumulus.org/fhir/code/manufacturingParticipantRole"/>
                           <code value="example"/>
                           <display value="StartingMaterial"/>
                        </coding>
                     </role>
                  </participant>
                  <participant>
                     <typeReference>
                        <extension url="http://hl7.org/fhir/uv/pharm-quality/StructureDefinition/Extension-manufacturing-participant-pq">
                           <valueReference>
                              <reference value="SubstanceDefinition/processingMaterial"/>
                           </valueReference>
                        </extension>
                     </typeReference>
                     <role>
                        <coding>
                           <system value="http://accumulus.org/fhir/code/manufacturingParticipantRole"/>
                           <code value="example"/>
                           <display value="ProcessingMaterial"/>
                        </coding>
                     </role>
                  </participant>
                  <participant>
                     <typeReference>
                        <extension url="http://hl7.org/fhir/uv/pharm-quality/StructureDefinition/Extension-manufacturing-participant-pq">
                           <valueReference>
                              <reference value="SubstanceDefinition/resultingMaterial"/>
                           </valueReference>
                        </extension>
                     </typeReference>
                     <role>
                        <coding>
                           <system value="http://accumulus.org/fhir/code/manufacturingParticipantRole"/>
                           <code value="example"/>
                           <display value="ResultingMaterial"/>
                        </coding>
                     </role>
                  </participant>
                  

               </action>
            </action>
         </PlanDefinition>
      </resource>
   </entry>
   <!-- Section 5.2 - Unit Operation/Equipment Information -->
   <entry>
