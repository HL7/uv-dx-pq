            </deviceName>
            <property>
               <type>
                  <coding>
                     <system value="http://accumulus.org/fhir/code/deviceProperty"/>
                     <code value="121212"/>
                     <!-- #90 Working capacity -->
                     <display value="Maximum Speed"/>
                  </coding>
               </type>
               <valueQuantity>
                  <value value="5000"/>
                  <unit value="RPM"/>
               </valueQuantity>
            </property>
         </DeviceDefinition>
