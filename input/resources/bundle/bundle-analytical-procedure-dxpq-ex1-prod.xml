<!-- Product Analytical Procedure -->
   <id value="bundle-analytical-procedure-dxpq-ex1-prod"/>
   <!--   <id value="product-analytical-procedure"/>-->
   <meta>
      <profile value="http://hl7.org/fhir/uv/dx-pq/StructureDefinition/Bundle-analytical-procedure-dxpq"/>
<!--      <profile value="http://accumulus.org/fhir/dx-cmc/analytical-procedure"/>-->
   </meta>
   <type value="collection"/>
   <entry>
      <fullUrl value="urn:uuid:2038e4c7-22b8-4c56-2d28-6c077648763f"/>
      <resource>
         <PlanDefinition>
            <id value="analyticalProcedure"/>
            <title value="Analytical Procedure"/>
            <type>
               <coding>
                  <system value="http://terminology.hl7.org/CodeSystem/plan-definition-type"/>
                  <code value="workflow-definition"/>
                  <display value="Workflow Definition"/>
               </coding>
               <coding>
                  <system value="http://accumulus.org/fhir/code/planType"/>
                  <code value="analytical-procedure"/>
                  <display value="Analytical Procedure"/>
               </coding>
            </type>
            <status value="active"/>
            <subjectReference>
               <reference value="MedicinalProductDefinition/medicinalproductdefinition-drug-product-dxpq-ex1"/>
            </subjectReference>
            <action>
               <title value="Preparation and Analysis of Drug Product Samples"/>
               <participant>
                  <typeReference>
                     <extension url="http://accumulus.org/fhir/extension/manufacturingParticipant">
                        <valueReference>
                           <reference value="SubstanceDefinition/diluent"/>
                        </valueReference>
                     </extension>
                  </typeReference>
                  <role>
                     <coding>
                        <system value="http://accumulus.org/fhir/code/manufacturingParticipantRole"/>
                        <code value="diluent"/>
                        <display value="Diluent"/>
                     </coding>
                  </role>
               </participant>
               <participant>
                  <typeReference>
                     <extension url="http://accumulus.org/fhir/extension/manufacturingParticipant">
                        <valueReference>
                           <reference value="SubstanceDefinition/resultingMaterial"/>
                        </valueReference>
                     </extension>
                  </typeReference>
                  <role>
                     <coding>
                        <system value="http://accumulus.org/fhir/code/manufacturingParticipantRole"/>
                        <code value="ResultingMaterial"/>
                        <display value="Resulting Material"/>
                     </coding>
                  </role>
               </participant>
               <action>
                  <title value="Sample Preparation"/>
                  <action>
                     <title value="Instructions"/>
                     <description value="Transfer 5 tablets into a volumetric flask. Add a portion of the diluent and shake on mechanical shaker until tablets are completely disintegrated. Dilute to volume with diluent. Do not exceed 2 mg/mL in the initial dilution. Filter or centrifuge."/>
                  </action>
               </action>
               <action>
                  <title value="Sample Analysis"/>
                  <action>
                     <code>
                        <text value="Test"/>
                     </code>
                     <definitionCanonical value="ObservationDefinition/SampleAnalysis"/>
                  </action>
               </action>
            </action>
         </PlanDefinition>
      </resource>
   </entry>
   <entry>
